{
    "parser": "@babel/eslint-parser",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "react/prop-types": "off",        
        "no-unused-vars": "warn",
        "no-console": "warn"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}